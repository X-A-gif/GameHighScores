<div class="flex h-screen z-1">
          <!-- Sidebar -->
          <div class="bg-gray-900 text-gray-100 flex flex-col justify-between w-64">
            <div class="px-4 py-8">
              <h1 class="text-2xl font-bold">Games</h1>
            </div>
            <nav class="flex-grow">
              <ul class="px-4">
                <li class="mb-2">
                  <a href="/game" class="block p-2 rounded hover:bg-gray-800">SpaceInvaders 1</a>
                </li>
           
              </ul>
            </nav>
            <div class="px-4 py-8">
              <p class="text-gray-500">Sidebar footer</p>
            </div>
          </div>

          <div class="flex-grow bg-gray-100 p-4">
              <div class="Scores">

              <div class="row">
                <div class="col-auto">
                  <h2>Welcome {{user.name}} !! to the Arcade !</h2>
                </div>
              </div>

              <div class="row mt-4">
                <div class="col-md-6">
                  <h3>Check out the Highest Scores !!</h3>

                  <table id="table" >
              <tr>
                  <th>UserID</th>
                  <th>UserName</th>
                  <th>HighScore</th>
              </tr>

              {{#each users as |user| }}


              <tr>
                  <td>{{user.id}}</td>
                  <td>{{user.name}}</td>
                  <td>{{user.max_score}}</td>
              </tr>
              {{/each}}
              </table>

                </div>    
              </div>

            <footer>
                <p>&copy; 2023 Space Invaders Profile. All rights reserved.</p>
            </footer>
          </div>
        </div>




  
<script src="./js/profile.js"></script>
